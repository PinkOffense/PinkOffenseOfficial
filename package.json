{
  "name": "pinkoffense-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /PinkOffenseOfficial/",
    "build:dev": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit --audit-level=high",
    "audit:fix": "npm audit fix",
    "ci": "npm run lint && npm run typecheck && npm run build",
    "deploy": "ng build --base-href /PinkOffenseOfficial/ && npx angular-cli-ghpages --dir=dist/pinkoffense-portfolio/browser"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@types/three": "^0.182.0",
    "marked": "^17.0.1",
    "ngx-markdown": "^21.0.1",
    "rxjs": "~7.8.0",
    "three": "^0.182.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.0",
    "angular-cli-ghpages": "^3.0.2",
    "angular-eslint": "^19.6.0",
    "eslint": "^9.27.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.32.1"
  }
}
